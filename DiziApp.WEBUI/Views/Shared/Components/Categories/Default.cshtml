@model List<Category>

<div class="list-group">
    <a href="/series/list"  class="list-group-item list-group-item-action">Tüm Filmler</a>

    @foreach (var category in Model)
    {
        <a asp-controller="series" asp-action="list" asp-route-id="@category.CategoryId"  class="list-group-item list-group-item-action">
            @category.Name
        </a>
    }
</div>